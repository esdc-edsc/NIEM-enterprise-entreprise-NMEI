	<xs:element name="EmployeeOccupationTextAbstract" abstract="true">
		<xs:annotation>
			<xs:documentation>A data concept for a general kind of job title of an
				employee.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MandatoryTextType">
		<xs:simpleContent>
			<xs:restriction base="nc:TextType">
				<xs:pattern value="(.|\n)+"/>
			</xs:restriction>
		</xs:simpleContent>
